.info-group{
    margin-top: 10px;
}
.Vtuzx_Input_Group_Center {
    border-radius: 10px;
}
.info-group_item{
    display: flex;
    justify-content: space-between;
    /* align-items: center; */
    padding: 0 14.5px;
    /* height: 60px;
    line-height: 60px; */
}
.info-group_item .item-subTitle{
    display: flex;
    /* justify-content: space-between; */
    align-items: center;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    font-size: 14px;
    height: 60px;
    line-height: 60px;
    /* width: 120px; */
}
.item-subTitle_tag{
    color: #E3211FFF;
    /* padding-right: 10px; */
}
.item-subTitle_text{
    color: #454545FF;
    margin-left: 10px;
}
.info-group_item .item-input{
    display: flex;
    align-items: center;
    flex: 1;
    /* height: 60px;
    line-height: 60px; */
    border-bottom: 1px solid #E8E8E8FF;
}
vtu-input{
    flex: 1;
}
.item-input_tag text{
    font-size: 14px;
    color: #191919FF;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
}
.item-input_tag image{
    width: 6.5px;
    height: 10.5px;
}

.info-group_item:last-child .item-input{
    border: none;
}

.info-group .info-group-title{
    display: flex;
    align-items: center;
    height: 52.5px;
    padding: 0 14.5px;
}
.info-group .info-group-title_tag{
    color: #E3211FFF;
    font-size: 14px;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    width: 30px;
    text-align: center;
    border-left: 4px solid #E3211FFF;
}
.info-group .info-group-image{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 14.5px 20px;
}
.item-upload{
    width: 166px;
    height: 100px;
    background:#FFF4F4FF;
    border: 0.5px solid #E5E5E5FF;
    position: relative;
}
.item-upload image{
    width: 166px;
    height: 100px;
}
.item-upload .item-upload_icon{
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translateX(-50%);
}
.item-upload .item-upload_icon image{
    width: 17px;
    height: 17px;
}
.item-upload .item-upload_text{
    position: absolute;
    bottom: 30%;
    left: 50%;
    transform: translateX(-50%);
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #191919FF;
    font-size: 12px;
    width: 110px;
    text-align: center;
}



.info-group .item-image{
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 86px;
    text-align: center;
}
.info-group .wrap-resource_add image{
    width: 35px;
    height: 30.5px;
}
.info-group .wrap-resource_add text{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #666666FF;
    font-size: 12px;
}
.info-group .wrap-resource_add .item-video{
    width: 300px;
    margin: 0 auto;
}
.info-group_button{
    background: tomato;
    color: #ffffff;
    text-align: center;
    height: 50px;
    line-height: 50px;
    font-size: 18px;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    margin-top: 20px;
    cursor: pointer;
}

.popup-input{
    margin: 20px 12.5px;
    border: 1px solid #0A6EE8FF;
    border-radius: 9px;
}
.popup-input_label{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #191919FF;
    font-size: 17px;
}
.popup-input_text{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #999999FF;
    font-size: 14px;
}
.popup-title{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 58px;
    line-height: 58px;
    border: 0.5px solid #E5E5E5FF;
    padding: 0 20.5px;
    font-size: 17px;
}
.popup-title_cancel{
    color: #666666FF;
}
.popup-title_confirm{
    color: #F0222FFF;
}

van-checkbox-group{
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    padding: 22px;
    height: 108px;
    overflow-y: auto;
}
van-checkbox{
    width: 185px;
    margin-bottom: 23px;
    font-size: 16px;
    color: #191919FF;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
}

.item-input-child{
    width: 100%;
}
.item-input-child van-radio-group{
    display: flex;
    align-items: center;
    height: 60px;
    line-height: 60px;
    border-bottom: 1px solid #E8E8E8FF;
}
.item-input-child van-radio-group van-radio{
    margin-right: 20px;
}

.input-child-wrap{
    height: 60px;
    line-height: 60px;
    border-bottom: 1px solid #E8E8E8FF;
    display: flex;
    align-items: center;
    justify-content: space-between;
    /* padding: 0 10px; */
    font-size: 14px;
}
.input-child-wrap:last-child{
    border: none;
}
.input-child-wrap .input-child-wrap_msg{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #191919FF;
    font-size: 14px;
    padding-right: 12px;
}
.input-child-wrap_subTitle{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #878787FF;
    font-size: 14px;
    /* padding-left: 11px; */
}

.item-input-child_text{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #878787FF;
    font-size: 14px;
}




.wharf-container{
    height: 100vh;
    display: flex;
    flex-direction: column;
}
.page-main{
    flex: 1;
    background: #ffffff;
}
.region-container{
    height: 100%;
    display: flex;
    flex-direction: column;
}
.region-list{
    flex: 1;
    padding: 0 20px;
    line-height: 35px;
    margin-top: 31px;
}
.region-title{
    font-family: PingFang-SC-Bold;
    font-weight: Bold;
    font-size: 16px;
    color: #191919FF;
    padding: 30px 20px;
}
.region-title-close{
    position: absolute;
    right: 30px;
    top: 80px;
}
.region-title-close image{
    width: 16px;
    height: 16px;
}
.region-crumbs{
    font-family: region-crumbs;
    font-weight: Medium;
    color: #191919FF;
    font-size: 14px;
    padding: 0 20px;
    display: flex;
    align-items: center;
}
.region-crumbs_item{
    margin-right: 37px;
    position: relative;
}
.region-crumbs_item.active::after{
    content: '';
    position: absolute;
    width: 100%;
    height: 2px;
    bottom: -7px;
    left: 50%;
    transform: translateX(-50%);
    background: #E3211FFF;
}
.region-item{
    display: flex;
}
.region-item-img{
    margin-right: 10px;
}
.region-item-img image{
    width: 12px;
    height: 12px;
}
.region-cell{
    border: 1px solid #f1f1f1;
    border-radius: 10px;
    padding: 16px 16px !important; 
}
.popup-input{
    margin: 20px 12.5px;
    border: 1px solid #0A6EE8;
    border-radius: 9px;
}
.popup-input_label{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #191919FF;
    font-size: 17px;
}
.popup-input_text{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #999999FF !important;
    font-size: 14px;
}

van-dialog .button-container{
    display: flex;
    justify-content: space-evenly;
    padding: 20px 31px;
}
van-dialog .button-container .button-item{
    border: 1px solid #A6A6A6;
    border-radius: 14.5px;
    color: #999999;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    font-size: 12px;
    width: 96px;
    height: 28.5px;
    text-align: center;
    line-height: 28.5px;
    cursor: pointer;
}
van-dialog .button-container .button-item.active{
    background: #F0222F;
    color: #FFFFFF;
    border: 1px solid;
}