.address-container{
    display: flex;
    flex-direction: column;
    height: 100%;
    background: #fff;
}
.address-list{
    flex: 1;
    margin: 0 28px;
}
.address-title{
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    margin: 29px 0 41px 20px;
    font-size: 16px;
    color: #191919;
}
.address-crumbs{
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    color: #191919;
    font-size: 14px;
    margin-left: 27px;
    display: flex;
    align-items: center;
    margin-bottom: 35px;
}
.address-crumbs_item{
    margin-right: 37px;
    position: relative;
}
.address-crumbs_item.active::after{
    content: '';
    position: absolute;
    width: 100%;
    height: 2px;
    background: #E3211F;
    bottom: -5px;
    left: 0;
}
.address-item{
    margin-bottom: 22px;
    display: flex;
    align-items: center;
}
.address-item_active{
    margin-right: 9px;
}
.address-item_active image{
    width: 12px;
    height: 12px;
}
.address-item_name{
    font-size: 13px;
    color: #191919;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
}
.region-cell{
    border: 1px solid #f1f1f1;
    border-radius: 10px;
    padding: 16px 16px !important; 
}
.popup-input{
    margin: 20px 12.5px;
    border: 1px solid #0A6EE8FF;
    border-radius: 9px;
}
.popup-input_label{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #191919FF;
    font-size: 17px;
}
.popup-input_text{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #999999FF;
    font-size: 14px;
}