.resources-container{
  height: 100%;
  display: flex;
  flex-direction: column;
}
.resources-state{
  margin-top: 34.5px;
  text-align: center;
}
.resources-state_img{
  margin-bottom: 14.5px;
}
.resources-state_img image{
  width: 50px;
  height: 50px;
}
.resources-state_title{
  color: #3A74AD;
  font-family: PingFang-SC-Medium;
  font-weight: Medium;
  font-size: 15px;
  margin-bottom: 5px;
}
.resources-state_describe{
  color: #3A74AD;
  font-family: PingFang-SC-Medium;
  font-weight: Medium;
  font-size: 15px;
}




.resources-info{
  flex: 1;
}
.resources-list{
  padding-left: 15px;
  margin-top: 40.5px;
}
.resources-item{
  margin-bottom: 20.5px;
  display: flex;
  align-items: center;
}
.resources-item_subtitle{
  font-family: PingFang-SC-Medium;
  font-weight: Medium;
  color: #454545;
  font-size: 14px;
  margin-right: 21px;
}
.resources-item_text{
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: #191919;
  font-size: 15px;
}